                        <th>Cl 35</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var result in Model.list_tracemetal_results)
                    {
                        <tr>
                            <td>@result.description </td>
                            <td>@result.acquisition_date</td>
                            <td>@result.qc_stat</td>
                            <td> @result.Be9 </td>
                            <td> @result.B10 </td>
                            <td> @result.Al_27 </td>
                            <td> @result.Ti_47 </td>
                            <td> @result.V_1_51 </td>
                            <td> @result.V_51 </td>
                            <td> @result.Sc_45 </td>
                            <td> @result.Cr_52 </td>
                            <td> @result.Mn_55 </td>
                            <td> @result.Fe_1_57 </td>
                            <td> @result.Fe_2_57 </td>
                            <td> @result.Co_59 </td>
                            <td> @result.Ni_60 </td>
                            <td> @result.Cu_65 </td>
                            <td> @result.Zn_66 </td>
                            <td> @result.Ge_72 </td>
                            <td> @result.As_75 </td>
                            <td> @result.As_1_75 </td>
                            <td> @result.Se_82 </td>
                            <td> @result.Sr_87 </td>
                            <td> @result.Mo_98 </td>
                            <td> @result.Rh_103 </td>
                            <td> @result.Ag_107 </td>
                            <td> @result.Cd_111 </td>
                            <td> @result.In_115 </td>
                            <td> @result.Sb_121 </td>
                            <td> @result.Sn_118 </td>
                            <td> @result.Ba_137 </td>
                            <td> @result.Tb_159 </td>
                            <td> @result.Tl_205 </td>
                            <td> @result.Pb_208 </td>
                            <td> @result.Th_232 </td>
                            <td> @result.U_238 </td>
                            <td> @result.Na_23 </td>
                            <td> @result.Mg_24 </td>
                            <td> @result.K_39 </td>
                            <td> @result.Ca_44 </td>
                            <td> @result.Sc_145 </td>
                            <td> @result.CI_35 </td>
                        </tr>
                    }
                </tbody>
            </table>
            }            
        </div>
    </div>
</div>